@model ApliWeb.Models.Mascota
@{
    ViewBag.Title = "Modificar Datos de Mascota";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.otro</h3>





<section class="content jumbotron">
    
    
        @using (Html.BeginForm("EditarMascota", "Home", FormMethod.Post))
        {
            <div class="form-group">
                @Html.HiddenFor(m => m.Id_Mascota)
                @Html.LabelFor(m => m.Nombre)
                @Html.TextBoxFor(m => m.Nombre)
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.Edad)
                @Html.TextBoxFor(m => m.Edad)
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.Descrip)
                @Html.TextBoxFor(m => m.Descrip )
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.CorreoContacto)
                @Html.TextBoxFor(m => m.CorreoContacto)
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.Adoptada)
                @Html.TextBoxFor(m => m.Adoptada)
            </div>
            <input type="submit" class="btn btn-primary" value="Enviar" />
        
    }
    </section>